{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/angular"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "backend/server.js"
    },
    {
      "src": "/.*",
      "dest": "frontend"
    }
  ]
}
